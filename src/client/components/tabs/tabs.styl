.window-controls
  margin: 5px 5px 5px 0
  flex-shrink: 0
  // display: inline-block
  // position absolute
  // right 5px
  // margin-left: auto
  -webkit-app-region no-drag
  // top 5px
  // z-index 200
  border-radius 0 0 3px 3px
  // padding 0 0 0 4px
  background #333

.window-control-box
  display inline-block
  width 30px
  height 30px
  color #ccc
  -webkit-app-region no-drag
  &:hover
    color #08c
    cursor pointer
    .icon-maximize
      border-color #08c
  .widnow-control-icon
    padding: 5px

.icon-maximize
  width 10px
  height 7px
  border 1px solid #ccc
  cursor pointer
  &.is-max
    height 6px
    position relative
    &:before
      position absolute
      content ''
      left -3px
      top 3px
      width 8px
      height 4px
      border 1px solid #ccc
      border-top none
      border-right none

.control-icon:hover
  color #fff
.tab-scroll-icon
  color #888
  &:hover
    color #aaa